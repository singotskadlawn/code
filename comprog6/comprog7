#include <stdio.h>

int main() {

    int b[10];
    int i;
    
    printf("Enter 10 Integers\n");
    for ( i = 0; i<10; i++)
    scanf("%d", &b[i]);

    for ( i = 0; i<10; i++)
     if(b[i]%2 ==0)
        {
             printf(" %d,", b[i]);
        }
   
    return 0;
}
