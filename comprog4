#include <stdio.h>

int main() {
    
    int array[] = {10, 20, 30, 40, 50};
    int sum = 0;

    
    int size = sizeof(array) / sizeof(array[0]);

    for (int i = 0; i < size; i++) {
        if (i % 2 != 0) { 
            sum += array[i];
        }
    }
    printf("The sum of integers located in the odd index is: %d\n", sum);

    return 0;
}
